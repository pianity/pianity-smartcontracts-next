{
    "name": "warp-fee",
    "version": "0.0.1",
    "description": "Rust implementation of the FEE contract standard",
    "main": "index.js",
    "author": "Eyal Chojnowski <eyal@pianity.com>",
    "license": "MIT",
    "scripts": {
        "build": "wasm-pack build --target nodejs --release --out-name rust-contract implementation",
        "build-types": "yarn build-types-json && yarn build-types-ts",
        "build-types-json": "RUN_GENERATE=1 cargo test run_generate -- --nocapture",
        "build-types-ts": "ts-node --transpileOnly ./scripts/json2ts.ts"
    },
    "engines": {
        "node": ">=16.5"
    },
    "dependencies": {
        "json-schema-to-typescript": "^11.0.1",
        "typescript": "^4.6.2"
    },
    "devDependencies": {
        "@types/node": "^18.6.3",
        "ts-node": "^10.9.1"
    }
}
